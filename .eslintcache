fect(() => {\n    // will only run once when the app componenet loads\n\n    auth.onAuthStateChanged((authUser) => {\n      // console.log('THE USER IS>>>',authUser);\n\n      if (authUser) {\n        // the user just logged in/the user was logged in\n        dispatch({\n          type: \"SET_USER\",\n          user: authUser,\n        });\n      } else {\n        // the user is logged out\n        dispatch({\n          type: \"SET_USER\",\n          user: null,\n        });\n      }\n    });\n  }, []);\n\n  return (\n    // BEM\n    <Router>\n      <div className=\"App\">\n        <Switch>\n          <Route path=\"/orders\">\n            <Header />\n            <OrdersContext.Provider value={{ orders, setOrders }}>\n              <Orders />\n            </OrdersContext.Provider>\n          </Route>\n          <Route path=\"/login\">\n            <Login />\n          </Route>\n\n          <Route path=\"/checkout\">\n            <Header />\n            <OrdersContext.Provider value={{ orders, setOrders }}>\n              <Checkout />\n            </OrdersContext.Provider>\n          </Route>\n\n          <Route path=\"/payment\">\n            <Header />\n            <OrdersContext.Provider value={{ orders, setOrders }}>\n              <Elements stripe={promise}>\n                <Payment />\n              </Elements>\n            </OrdersContext.Provider>\n          </Route>\n\n          <Route path=\"/\">\n            <Header />\n\n            <Home />\n          </Route>\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","C:\\React apps\\Git\\amazon-clone\\src\\reportWebVitals.js",[],"C:\\React apps\\Git\\amazon-clone\\src\\Header.js",["183"],"C:\\React apps\\Git\\amazon-clone\\src\\Home.js",["184","185","186","187","188"],"import React from \"react\";\r\nimport { useState, useEffect } from \"react\";\r\nimport { db } from \"./firebase\";\r\nimport \"./Home.css\";\r\nimport Product from \"./Product\";\r\nimport CategoryFilter from \"